            $("#1_unidade").attr('class', 'inactive');
            $("#2_unidades").attr('class', 'active');
            $("#3_unidades").attr('class', 'inactive');

            $("#bola1").attr('style', '');
            $("#bola2").attr('style', 'background-color: var(--accent-color)');
            $("#bola3").attr('style', '');

