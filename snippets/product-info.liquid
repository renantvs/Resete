        function alterarVariante(variante){
          $( "input[value='"+variante+"']" ).click();

          if(variante == "1 unidade"){

            $("#1_unidade").attr('class', 'active');
            $("#2_unidades").attr('class', 'inactive');
            $("#3_unidades").attr('class', 'inactive');

